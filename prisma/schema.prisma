  @@index([parent_id], map: "payload_locked_documents_rels_parent_idx")
  @@index([partners_carousel_settings_id], map: "payload_locked_documents_rels_partners_carousel_settings_idx")
  @@index([path])
  @@index([section_titles_id])
  @@index([social_links_id])
  @@index([tenant_link_configuration_id], map: "payload_locked_documents_rels_tenant_link_configuration__idx")
  @@index([tenants_id])
  @@index([users_id])
}

model payload_migrations {
  id         String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  name       String?  @db.VarChar
  batch      Decimal? @db.Decimal
  updated_at DateTime @default(now()) @db.Timestamptz(3)
  created_at DateTime @default(now()) @db.Timestamptz(3)

  @@index([created_at])
  @@index([updated_at])
}

model payload_preferences {
  id                       String                     @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  key                      String?                    @db.VarChar
  value                    Json?
  updated_at               DateTime                   @default(now()) @db.Timestamptz(3)
  created_at               DateTime                   @default(now()) @db.Timestamptz(3)
  payload_preferences_rels payload_preferences_rels[]

